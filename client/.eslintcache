 => {\n    // delete in the db\n    axios.delete(`/api/blogs/${id}`)\n    .then( res => {\n      // delete in the state\n      const { blogs } = this.state\n        this.setState({ blogs: blogs.filter( b => b.id !== id)})\n        window.location.href = '/myblog'\n        // return <Redirect to='/myblog' />\n    })\n    .catch( err => {\n      console.log(err);\n    })\n  }\n\n  render() {\n    return(\n      <BlogContext.Provider value={{\n        ...this.state,\n        addBlog: this.addBlog,\n        updateBlog: this.updateBlog,\n        deleteBlog: this.deleteBlog,\n      }}>\n        { this.props.children }\n      </BlogContext.Provider>\n    )\n  }\n}\n\nexport default BlogProvider;","/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/index.js",[],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/Header/index.js",[],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/containers/Home/index.js",["88","89"],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/Hero/index.js",[],["90","91"],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/posts/PostForm.js",["92","93"],"import { Component } from 'react';\nimport { Form } from 'semantic-ui-react';\n\nclass PostForm extends Component {\n  state = { title: '', body: '', pic: '' }\n\n  componentDidMount() {\n    if (this.props.id) {\n      const { title, body, pic } = this.props\n      this.setState({ title, body, pic })\n    }\n  }\n\n  handleChange = (e) => {\n    const { name, value } = e.target\n    this.setState({ [name]: value })\n  }\n\n  handleSubmit = (e) => {\n    e.preventDefault();\n    if (this.props.id) {\n      const { blogId, updatepost, id, setOpen } = this.props\n      updatepost(blogId, id, this.state)\n    } else {\n      const { addPost, blogId } = this.props\n      addPost(blogId, this.state)\n    }\n    this.setState({ title: '', body: '', pic: '' })\n  }\n\n  render() {\n    const { title, body, pic } = this.state \n    return(\n      <Form onSubmit={this.handleSubmit}>\n        <Form.Input\n          name='title'\n          value={title}\n          onChange={this.handleChange}\n          required\n          label='Title'\n        />\n        <Form.Input\n          name='body'\n          value={body}\n          onChange={this.handleChange}\n          required\n          label='Text'\n          type=\"text\" \n        />\n        <Form.Button>\n          Submit\n        </Form.Button>\n      </Form>\n    )\n  }\n}\n\nexport default PostForm;\n","/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/providers/PostProvider.js",[],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/App.js",["94","95","96"],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/containers/AboutMe/index.js",[],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/containers/ProgrammingPortfolio/index.js",["97","98","99","100"],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/posts/PostList.js",[],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/Logo/index.js",["101"],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/Navbar/index.js",["102","103","104"],"/Users/Parker/Desktop/dpl_winter20/myblog_myportfoliowinter20/client/src/components/UI/Card/index.js",[],{"ruleId":"105","severity":1,"message":"106","line":1,"column":10,"nodeType":"107","messageId":"108","endLine":1,"endColumn":14},{"ruleId":"105","severity":1,"message":"109","line":2,"column":10,"nodeType":"107","messageId":"108","endLine":2,"endColumn":14},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"110","replacedBy":"114"},{"ruleId":"112","replacedBy":"115"},{"ruleId":"105","severity":1,"message":"116","line":13,"column":49,"nodeType":"107","messageId":"108",